{
  "name": "formpages-io-master",
  "version": "1.0.0",
  "description": "a simple form sanitation ",
  "main": "index.js",
  "targets": {
    "main": false
  },
  "source": "src/index.html",
  "scripts": {
    "start": "parcel src/index.html --dist-dir dist ",
    "build": "parcel build src/index.html --dist-dir  docs --public-url ./",
    "deploy": "npm run build && git add . && git commit -a -m 'latest build' &&  git push -u origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlejandroBryan/formpages-io.git"
  },
  "author": "Alejandro Bryan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlejandroBryan/formpages-io/issues"
  },
  "homepage": "https://github.com/AlejandroBryan/formpages-io#readme",
  "devDependencies": {
    "parcel": "^2.0.1"
  }
}
